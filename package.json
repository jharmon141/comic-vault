    "vue-router": "^2.4.0",
    "vuex": "^2.2.1"
  },
  "devDependencies": {
    "apollo-client": "^0.5.26",
    "vue": "^2.1.10",
    "vue-apollo": "^1.3.0",
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chalk": "^1.1.3",
